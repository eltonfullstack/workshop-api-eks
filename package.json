    "build": "npm run swagger:gen && node esbuild.config.js",
    "build:prod": "tsc",
    "build:watch": "node esbuild.config.js --watch",
    "type-check": "tsc --noEmit",
    "start": "node dist/server.js",
    "start:prod": "node dist/src/server.js",
    "serve": "nodemon dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "export NODE_ENV=local && jest",
    "tdd": "export NODE_ENV=local && jest --watch",
    "test:watch": "jest --watch",
    "coverage": "export NODE_ENV=local && npm test -- --coverage",
    "coverage:unit": "export NODE_ENV=local && npm test -- --coverage --testPathIgnorePatterns='__tests__/integration'",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "db:generate": "npx prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force",
    "prepare": "npm run db:generate",
    "swagger:gen": "tsx docs/generate-swagger.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
